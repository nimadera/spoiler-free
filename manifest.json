{
	"manifest_version": 2,

	"name": "Spoiler Free",
	"description": "This extension removes user selected words from the DOM",
	"version": "1.0",

	"browser_action": {
		"default_icon": "images/spoilerfree128.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage"
	]/*,
	"icons": {
		"64": "images/spoilerfree64.png"
	},
	"background": {
    	"scripts": ["background.js"],
    	"persistent": false
  	}*/
}